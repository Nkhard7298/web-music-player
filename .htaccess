#rewriting
RewriteEngine on
RewriteRule ^$                   /display/player.html [L]
RewriteRule ^sign$               /display/index.html [L]
RewriteRule ^sign-out$           /display/sign-out.html [L]
RewriteRule ^catalog$            /display/catalog.html [L]
RewriteRule ^library$            /display/library.html [L]
RewriteRule ^albums$             /display/album.html [L]
RewriteRule ^artists$            /display/artist.html [L]
RewriteRule ^profile$            /display/profile.html [L]
RewriteRule ^users$              /display/users.html [L]
RewriteRule ^stream/([0-9]+)$    /server/track.php?track=$1 [QSA,L]
RewriteRule ^server/covers/([0-9]+).jpeg$    /display/files/images/default_cover.png [QSA,L] #dummy line before develop #8
